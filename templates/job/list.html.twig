{% extends 'base.html.twig' %}

{% block body %}
<table class="table table-hover table-striped" border="1">

<thead>
<tr>
<th>#</th>
<th>type</th>
<th> company </th>
<th> description </th>
<th>email</th>
<th>image</th>
<th></th>
</thead>
<tbody>
{% for job in lesJob %}
<tr>
 <td>{{job.id}}</td>
<td>{{job.type}}</td>
<td>{{job.company}}</td>
<td>{{job.description}}</td>
<td>{{job.email}}</td>

<td> {% if job.image is not null %}
    <img src="{{job.image.url}}" alt="{{job.image.alt}}" height="100">
    {% endif %}</td>



<td>
<a class="btn btn-primary" href="#">Voir</a>
<a class="btn btn-info" href="#">Modifier</a>
<a class="btn btn-danger" href="#">Supprimer</a>
</td>
</tr>
{% endfor %}
</tbody>
</table>


{%  endblock  %}